package basic;

public class Variable {
	public static void main(String[] args) {
		// data type:-)
	String name = "siva";
	int number = 123;
	long count= 100000;
	char letter= 'a';
	double decemal = 10.2;
	short value= 100;
	float x= 10f;
	System.out.println(name+number+count+letter+decemal+value+x);
	
	}
	
public void sivaLearningToCode() {
	int a = 10;
	int b = 20;
	System.out.println("Addition of a and b is "+ (a+b));
	
}
void sivaMultiplying() {
			// TODO multiplying
	short num1= 15;
	short num2 = 3;
	System.out.println("multiplication of num1 and num2 is "+ (num1*num2));
	

}

}
