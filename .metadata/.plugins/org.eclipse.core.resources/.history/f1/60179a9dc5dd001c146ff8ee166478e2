package basic;

public class Constructor {  
	int number;
	String name;
	double num1;
	short value;
	float x;
	boolean b;
	long L;
	byte by;
	

	
public Constructor(int number) {
	this.number= number;
	
System.out.println(num1);
System.out.println(name);
	
	
}
void display() {
	
}
public static void main(String[] args) {
	Constructor c = new Constructor(100);
	
	c.display();

}
}
